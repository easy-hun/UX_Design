<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#65C869"
    tools:context=".MainActivity_menu2">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="59dp"
        android:text="분류 별 상품 모아보기"
        android:textAlignment="center"
        android:textColor="#17601A"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="63dp"
        android:backgroundTint="#216124"
        android:text="적용하기"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="184dp"
        android:layout_marginBottom="66dp"
        android:text="남성"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:chipBackgroundColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/chip3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip3"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="71dp"
        android:text="상의"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:chipBackgroundColor="@color/purple_200"
        app:layout_constraintBottom_toTopOf="@+id/chip4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chip" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip4"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="215dp"
        android:text="여름"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:chipBackgroundColor="@color/purple_500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chip3" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip2"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="66dp"
        android:text="여성"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:chipBackgroundColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/chip5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip5"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="71dp"
        android:text="하의"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:chipBackgroundColor="@color/purple_200"
        app:layout_constraintBottom_toTopOf="@+id/chip6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chip2" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip6"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="215dp"
        android:text="겨울"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:chipBackgroundColor="@color/purple_500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chip5" />
</androidx.constraintlayout.widget.ConstraintLayout>